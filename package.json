{
  "name": "@terrestris/base-util",
  "version": "0.1.1",
  "description": "A set of helper classes for working with strings, objects, etc.",
  "module": "src/index.js",
  "main": "dist/index.js",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "clean:dist": "rimraf ./dist/*",
    "test": "jest --maxWorkers=4",
    "test:watch": "jest --watchAll",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run build:dist",
    "build:dist": "npm run clean:dist && babel src --out-dir dist --copy-files --ignore spec.js",
    "release": "np --no-yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terrestris/base-util.git"
  },
  "author": "terrestris GmbH & Co. KG <info@terrestris.de>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/terrestris/base-util/issues"
  },
  "homepage": "https://github.com/terrestris/base-util#readme",
  "dependencies": {
    "lodash": "4.17.10",
    "loglevel": "1.6.1",
    "query-string": "6.1.0",
    "url-parse": "1.4.3",
    "validator": "10.6.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.2",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-0": "6.24.1",
    "coveralls": "3.0.2",
    "jest": "23.5.0",
    "np": "3.0.4",
    "whatwg-fetch": "2.0.4"
  }
}
